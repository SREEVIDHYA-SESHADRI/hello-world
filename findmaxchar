import java.io.*;
import java.util.*;
class maxcharinstr
{
public static void main(String arg[])
{
	int ind;
	String[] st=new String[2]; 
	Scanner s=new Scanner(System.in);
	
	for(int i=0;i<2;i++)
	{
	st[i]=s.next();
	}
	int max=st[0].length();
	for(int i=0;i<2;i++)
	{
		if(st[i].length()>max)
			{
			max=st[i].length();
			ind=i;
			}
		else if(st[i].length()==max)
			{
				continue;
			}
			
	}
	System.out.println(max);
}
}
	
	
